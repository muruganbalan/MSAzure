{
	"name": "PL_PROCESS_TRANSACTION",
	"properties": {
		"description": "This pipeline is for extracting Nested JSON data from Azure Blob Storage for Data Cleansing.",
		"activities": [
			{
				"name": "ACT_COPYFROM_BLOB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"sink": {
									"name": ""
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_ABLB",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_CSV_TRANSACTION",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}