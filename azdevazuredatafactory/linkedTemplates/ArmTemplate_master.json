{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azdevazuredatafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_ABLB_MOMENTIVE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_MOMENTIVE'"
		},
		"LS_ABLB_TRANSACTIONS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_TRANSACTIONS'"
		},
		"LS_ADB_CON_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_ADB_CON'"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_ASQL_SRC_OLTP_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL_SRC_OLTP'"
		},
		"LS_ASQL_TGT_OLAP_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL_TGT_OLAP'"
		},
		"LS_ONPREMSQL_SOURCE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ONPREMSQL_SOURCE'"
		},
		"DS_ABLB_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Transaction.json"
		},
		"DS_ABLB_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "publicrestapi/sample"
		},
		"ds_json_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Customer.json"
		},
		"ds_json_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "publicrestapi/sample"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdevadls.dfs.core.windows.net"
		},
		"Web1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.cognizant.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_ABLB_MOMENTIVE_connectionString": {
						"value": "[parameters('LS_ABLB_MOMENTIVE_connectionString')]"
					},
					"LS_ABLB_TRANSACTIONS_connectionString": {
						"value": "[parameters('LS_ABLB_TRANSACTIONS_connectionString')]"
					},
					"LS_ADB_CON_accessToken": {
						"value": "[parameters('LS_ADB_CON_accessToken')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_ASQL_SRC_OLTP_connectionString": {
						"value": "[parameters('LS_ASQL_SRC_OLTP_connectionString')]"
					},
					"LS_ASQL_TGT_OLAP_connectionString": {
						"value": "[parameters('LS_ASQL_TGT_OLAP_connectionString')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"DS_ABLB_properties_typeProperties_fileName": {
						"value": "[parameters('DS_ABLB_properties_typeProperties_fileName')]"
					},
					"DS_ABLB_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_ABLB_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ONPREMSQL_SOURCE_connectionString": {
						"value": "[parameters('LS_ONPREMSQL_SOURCE_connectionString')]"
					},
					"ds_json_properties_typeProperties_fileName": {
						"value": "[parameters('ds_json_properties_typeProperties_fileName')]"
					},
					"ds_json_properties_typeProperties_folderPath": {
						"value": "[parameters('ds_json_properties_typeProperties_folderPath')]"
					},
					"Web1_properties_typeProperties_url": {
						"value": "[parameters('Web1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		}
	]
}